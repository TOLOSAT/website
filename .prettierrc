{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"semi": false,
	"tabWidth": 2,
	"printWidth": 80,
	"htmlWhitespaceSensitivity": "css",
	"bracketSameLine": false,
	"proseWrap": "always",
	"endOfLine": "crlf",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-css-order"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
